<% provide(:title, @doctor.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= @doctor.name %>
      </h1>
      <p><%= @doctor.desc %>
      <% if @doctor.company %>
      <p><%= link_to @doctor.company.name, @doctor.company %>
      <% end %>
    </section>
  </aside>
</div>

